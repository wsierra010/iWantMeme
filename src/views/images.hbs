{{#each images}}

    <div class="col-md-6">
        <div class="row">
            <div class="item col-sm-6 col-md-4 mb-3">
                <img src="{{imageURL}}" alt="" width="100%" height="100%">
            </div>
        </div>
    </div>
    {{else}}
        <div class="col-md-4 mx-auto">
            <div class="card card-body text-center">
                <p>There are not memes save yet.</p>
                <a href="/images/upload">Upload One</a>
            </div>
        </div>
{{/each}}